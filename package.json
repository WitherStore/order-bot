{
  "name": "witherstore-bot",
  "version": "1.0.0",
  "main": "build/index.js",
  "type": "module",
  "scripts": {
    "build": "esbuild ./src/index.ts --sourcemap --format=esm --bundle --minify --platform=node --target=node24 --external:discord.js --external:stripe --external:source-map-support --outdir=build/",
    "start": "node --env-file=.env.local .",
    "dev": "nodemon -w src -e ts -x \"pnpm build && pnpm start\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.12.4",
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "esbuild": "^0.25.9",
    "eslint": "^9.33.0",
    "nodemon": "^3.1.10",
    "source-map-support": "^0.5.21",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "discord.js": "^14.21.0",
    "stripe": "^18.4.0",
    "zod": "^4.0.0"
  }
}
